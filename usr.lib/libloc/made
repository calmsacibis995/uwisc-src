cc -O  -c setenv.c
/lib/cpp -E  bset.s | as -o bset.o
ld -x -r bset.o
mv a.out bset.o
ar cvr libloc.a any.o atob.o copy.o getword.o isnumber.o isblank.o  ltrunc.o pmatch.o strdup.o strecat.o stricmp.o to_lower.o  instr.o iinstr.o fsleep.o ptypair.o setenv.o bset.o
